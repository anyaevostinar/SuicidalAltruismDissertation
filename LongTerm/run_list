set class_pref 95
set walltime 4
set mem_request 3

##EXperiments (Starting with Not)
51..80 explode_not_long_1k ./avida -s $seed -set COPY_MUT_PROB .0002 -set WORLD_X 25 -set WORLD_Y 40 -set EVENT_FILE events-1k.cfg -def INST_SET instset-heads.cfg -set ENVIRONMENT_FILE environment.cfg -set KABOOM_RADIUS 2 -set KABOOM_HAMMING 5 -set KABOOM_PROB .05 -set NO_MUT_INSTS 'A'

